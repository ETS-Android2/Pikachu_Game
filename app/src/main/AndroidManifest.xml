        android:supportsRtl="true"
        android:theme="@style/Theme.Pikachu_Game">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyDvF008XdfsMI2--XA2ySRk70GYaqFaTtw" />

        <activity
            android:name=".Activity_TopTen"
            android:exported="true"
            />
        <activity
            android:name=".Activity_Start"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Activity_Game"
            android:exported="true"
         />

        <activity
            android:name=".Activity_GameOver"
            android:exported="true"
        />

